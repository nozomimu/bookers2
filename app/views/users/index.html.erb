  <div class='col-xs-9'>
    <h2>Users</h2>

    <table class='table'>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
      <tr>
        <td><img src="/no_image.jpg" alt="No image" /></td>

     <% @users.each do |user| %>

     <%= user.name %><br>
       <span>
        <%=link_to user.title, "/users/#{user.id}" %></span>
     <% end %>
      </tr>

  </tbody>
</html>
